{
    "train_data_path": "work/dataset/quizbowl/train_sentence.json",
    "dataset_reader": {
        "type": "text_entity",
        "tokenizer": {
            "type": "pretrained_transformer",
            "model_name": "bert-large-cased",
            "max_length": 64
        },
        "token_indexers": {
            "tokens": {
                "type": "pretrained_transformer",
                "model_name": "bert-large-cased"
            }
        }
    },
    "model": {
        "type": "quiz",
        "text_field_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "pretrained_transformer",
                    "model_name": "bert-large-cased"
                }
            }
        },
        "seq2vec_encoder": {
            "type": "cls_pooler",
            "embedding_dim": 1024,
            "cls_is_last_token": false
        },
        "dropout": 0.1
    },
    "data_loader": {
        "batch_sampler": {
            "type": "bucket",
            "batch_size": 1
        }
    },
    "trainer": {
        "optimizer": null
    }
}
